<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Frame name="ML_TitleBar" virtual="true">
        <Size x="200" y="30" />
        <Layers>
            <Layer level="BACKGROUND">
                <Texture>
                    <Color r="0" g="0" b="0" a="1" />
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="Label" inherits="GameFontHighlightLarge" wordwrap="false"
                    justifyH="CENTER" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>